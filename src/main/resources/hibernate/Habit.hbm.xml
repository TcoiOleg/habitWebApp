<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.habit.custom.server.api.model.Habit" table="stock" catalog="habit">
        <id name="id" type="java.lang.Long">
            <column name="idhabit" />
            <generator class="identity" />
        </id>
        <property name="name" type="string">
            <column name="name" length="10" not-null="true" unique="true" />
        </property>
        <property name="score" type="integer">
            <column name="score" length="20" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>